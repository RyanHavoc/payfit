{
   "name":"processor",
   "version":"0.1.0",
   "private":true,
   "engines":{
      "node":">=9.11"
   },
   "scripts":{
      "start":"yarn check-prerequisites && concurrently \" yarn start:api \" \" yarn start:client \" \" yarn watch:scss \" -n \" Server,Client:JS,Client:CSS \" -c \" green,magenta,blue \" ",
      "start:api":"ts-node-dev --transpileOnly --no-notify api/index.ts",
      "start:client":"kill-port 3000 & react-scripts start",
      "watch:scss":"npm run build:scss & node-sass-chokidar src/ -o src/ --watch --recursive",
      "build:api":"rimraf dist & tsc --allowJs --outDir dist --sourceMap false",
      "build:scss":"node-sass-chokidar src/ -o src/",
      "build:client":"concurrently \" yarn build:scss \" \" react-scripts build \" ",
      "serve:api":"node dist/index.js",
      "test":"NODE_ENV=test NODE_PATH=src/ jest 'src/|api/' --bail",
      "test:api":"NODE_ENV=test JEST_PROJECT=api jest 'api/' --bail",
      "test:api:watch":"yarn test:api --watch",
      "test:client":"NODE_ENV=test NODE_PATH=src/ JEST_PROJECT=client jest 'src/' --bail",
      "test:client:watch":"yarn test:client --watch",
      "flow":"flow --show-all-errors",
      "lint":"yarn lint:api & yarn lint:client",
      "lint:client":"esprint check",
      "lint:api":"tslint -c tslint.json 'api/**/**/*.ts'",
      "format":"yarn format:api & yarn format:client",
      "format:client":"prettier 'src/**/*.{js,css,json}' --write",
      "format:api":"prettier 'api/**/*.ts' --write",
      "type:check":"tsc --noEmit",
      "check-prerequisites":"check-node-version --package"
   },
